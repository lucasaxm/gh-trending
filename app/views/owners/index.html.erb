<p id="notice"><%= notice %></p>

<h1>Owners</h1>

<table>
  <thead>
    <tr>
      <th>Nodeid</th>
      <th>Login</th>
      <th>Name</th>
      <th>Type</th>
      <th>Avatar url</th>
      <th>Html url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @owners.each do |owner| %>
      <tr>
        <td><%= owner.nodeid %></td>
        <td><%= owner.login %></td>
        <td><%= owner.name %></td>
        <td><%= owner.type_id %></td>
        <td><%= owner.avatar_url %></td>
        <td><%= owner.html_url %></td>
        <td><%= link_to 'Show', owner %></td>
        <td><%= link_to 'Edit', edit_owner_path(owner) %></td>
        <td><%= link_to 'Destroy', owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Owner', new_owner_path %>
